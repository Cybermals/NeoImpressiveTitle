#:kivy 1.9.0

<BioDialog>:
    title: "Bio"
    pos: 0, 60
    size_hint: .25, .6

    RelativeLayout:
        pos: root.pos
        width: root.width
        height: root.height - root.title_bar.height

        BoxLayout:
            padding: dp(12)

            GameTextArea:
                on_text: root.set_modified(True)
